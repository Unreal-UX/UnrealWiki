          <div class="flex-items-center flex-wrap d-inline-flex f6 my-1">
              <a class="topic-tag topic-tag-link f6 my-1" href="/search?q=topic%3Areact+org%3Athe-road-to-learn-react&amp;type=Repositories" title="Topic: react" data-ga-click="Topic, repository list" data-octo-click="topic_click" data-octo-dimensions="topic:react,repository_id:188193224,repository_nwo:the-road-to-learn-react/react-reusable-components,repository_public:true,repository_is_fork:false">
